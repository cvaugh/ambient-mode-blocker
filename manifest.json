{
    "manifest_version": 2,
    "version": "1.0",
    "name": "Ambient Mode Blocker",
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_start"
        }
    ]
}